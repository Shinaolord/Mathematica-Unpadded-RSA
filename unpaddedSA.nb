Notebook[{
Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"rsa200", "[", 
     RowBox[{
      RowBox[{"minimumdiff_", "?", "IntegerQ"}], ",", 
      RowBox[{"printQ_:", "True"}]}], "]"}], "/;", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"minimumdiff", "\[GreaterEqual]", " ", "0"}], ",", "True", ",", 
      RowBox[{
       RowBox[{"Message", "[", 
        RowBox[{
         RowBox[{"rsa200", "::", "mdiff"}], ",", "minimumdiff"}], "]"}], ";", 
       " ", "False"}]}], "]"}]}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"p", "=", 
        RowBox[{"Prime", "[", "5", "]"}]}], ",", 
       RowBox[{"q", "=", 
        RowBox[{"Prime", "[", "6", "]"}]}], ",", "pubkey", ",", "phi", ",", 
       "t", ",", "e", ",", "d", ",", 
       RowBox[{"j", "=", "1"}], ",", 
       RowBox[{"md", "=", "minimumdiff"}]}], "}"}], ",", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"j", "\[LessEqual]", "md"}], "  ", "||", " ", 
         RowBox[{
          RowBox[{"IntegerLength", "[", 
           RowBox[{"p", "*", "q"}], "]"}], "\[NotEqual]", " ", "400"}]}], ",",
         "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"p", "=", 
          RowBox[{"NextPrime", "[", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"10", "^", "199"}], ",", 
              RowBox[{"10", "^", "200"}]}], "}"}], "]"}], "]"}]}], ";", " ", 
         RowBox[{"(*", " ", 
          RowBox[{"Could", " ", "also", " ", "use", " ", 
           RowBox[{"RandomPrime", "[", "]"}]}], " ", "*)"}], 
         "\[IndentingNewLine]", 
         RowBox[{"q", "=", 
          RowBox[{"NextPrime", "[", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"10", "^", "199"}], ",", 
              RowBox[{"10", "^", "200"}]}], "}"}], "]"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"j", "=", 
          RowBox[{"Abs", "[", 
           RowBox[{"p", "-", "q"}], "]"}]}], ";"}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"pubkey", "=", 
       RowBox[{"p", "*", "q"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"phi", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"p", "-", "1"}], ")"}], 
        RowBox[{"(", 
         RowBox[{"q", "-", "1"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"t", "=", "False"}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"t", "==", "False"}], ",", " ", 
        RowBox[{
         RowBox[{"e", "=", 
          RowBox[{"RandomInteger", "[", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"10", "^", "100"}], ",", 
             RowBox[{"10", "^", "400"}]}], "}"}], "]"}]}], ";", " ", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"GCD", "[", 
             RowBox[{"e", ",", "phi"}], "]"}], "\[Equal]", "1"}], ",", 
           RowBox[{"t", "=", "True"}]}], "]"}]}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"d", "=", 
       RowBox[{"PowerMod", "[", 
        RowBox[{"e", ",", 
         RowBox[{"-", "1"}], ",", "phi"}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"printQ", "\[Equal]", "True"}], ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"Print", "[", 
           RowBox[{"\"\<p=\>\"", ",", "p"}], "]"}], " ", ",", 
          RowBox[{"Print", "[", 
           RowBox[{"\"\<q=\>\"", ",", "q"}], "]"}], ",", 
          RowBox[{"Print", "[", 
           RowBox[{"\"\<pubkey=\>\"", ",", "pubkey"}], "]"}], ",", 
          RowBox[{"Print", "[", 
           RowBox[{"\"\<e=\>\"", ",", "e"}], "]"}], ",", 
          RowBox[{"Print", "[", 
           RowBox[{"\"\<d=\>\"", ",", "d"}], "]"}], ",", 
          RowBox[{"Print", "[", 
           RowBox[{
           "\"\<\!\(\*SubscriptBox[\(mod\), \(\[Phi]\)]\)(e*d)=\>\"", ",", 
            RowBox[{"Mod", "[", 
             RowBox[{
              RowBox[{"e", "*", "d"}], ",", "phi"}], "]"}]}], "]"}], ",", 
          RowBox[{"Print", "[", 
           RowBox[{"\"\<diff=\>\"", ",", "j"}], "]"}]}], "}"}], ",", 
        "Nothing"}], "]"}], ";", "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"p", ",", "q", ",", "pubkey", ",", "e", ",", "d", ",", 
        RowBox[{"Mod", "[", 
         RowBox[{
          RowBox[{"e", "*", "d"}], ",", "phi"}], "]"}], ",", "j"}], "}"}]}]}],
     "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"rsa200", "::", "mdiff"}], "=", 
   "\"\<The minimum difference cannot be negative\>\""}], ";"}]}], "Code"]
